                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text Рубин
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text Топаз
                .catalog-menu__inner
                    .catalog-menu__title Проба
                    form.catalog-menu__items
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text 585
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox" checked)
                            span.catalog-menu__text 750
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text 925
                .catalog-menu__inner
                    .catalog-menu__title Наличие
                    form.catalog-menu__items
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text Есть в наличии
            .catalog-sort
                span.catalog-sort__title Сортировать по:
                form.catalog-sort__items
                    label.catalog-sort__item 
                        input.catalog-sort__input(type="checkbox")
                        span.catalog-sort__text Популярности
                    label.catalog-sort__item 
                        input.catalog-sort__input(type="checkbox")
                        span.catalog-sort__text По новизне
                    label.catalog-sort__item 
                        input.catalog-sort__input(type="checkbox")
                        span.catalog-sort__text По цене
