extends layout/layout

block vars
    - var pageTitle = 'Diplom'
    - var pageBodyClass = ''
    - var pageHeaderClass = ''

block content
    section.catalog
        .container 
            p.catalog__bread Ювелирные украшения  /  Кольца
            h2.h2.catalog__title Кольца
            .catalog-menu
                .catalog-menu__inner
                    .catalog-menu__title Категория
                    form.catalog-menu__items
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox" checked)
                            span.catalog-menu__text Кольца
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text Серьги
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text Подвески
                .catalog-menu__inner
                    .catalog-menu__title Материал
                    form.catalog-menu__items
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox" checked)
                            span.catalog-menu__text Золото
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
                            span.catalog-menu__text Серебро
                .catalog-menu__inner
                    .catalog-menu__title Вставка
                    form.catalog-menu__items
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox" checked)
                            span.catalog-menu__text Бриллиант
                        label.catalog-menu__item
                            input.catalog-menu__input(type="checkbox")
